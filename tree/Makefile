tree:main.o CreateBiTree.o PreOrderTraverse.o InOrderTraverse.o PostOrderTraverse.o GetMaxDepth.o LeafNodeNum.o LevelNodeNum.o CountNode.o SearchNode.o CopyBiTree.o DestroyBiTree.o BST.o HuffmanTree.o
	gcc -o tree main.o CreateBiTree.o PreOrderTraverse.o InOrderTraverse.o PostOrderTraverse.o GetMaxDepth.o LeafNodeNum.o LevelNodeNum.o CountNode.o SearchNode.o CopyBiTree.o DestroyBiTree.o BST.o HuffmanTree.o

main.o:
	gcc -c main.c -o main.o

CreateBiTree.o:
	gcc -c CreateBiTree.c -o CreateBiTree.o

